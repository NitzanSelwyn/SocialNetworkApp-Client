@model SocialNetworkClient.Models.MainModel

@{
    ViewBag.Title = "SearchedUsers";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Searched Users</h2>
@foreach (var item in Model.SearchedUsers)
{
    @Html.ActionLink($"{item.FirstName} {item.LastName}","ViewUser",item.Username)
    <br /> <br />
}
